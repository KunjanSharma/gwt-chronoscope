<module rename-to="gviz">

    <inherits name='com.google.gwt.user.User'/>
    <inherits name='org.timepedia.chronoscope.Chronoscope'/>
    <inherits name="com.google.gwt.gadgets.Gadgets"/>
    <inherits name="com.google.gwt.http.HTTP"/>
    <inherits name='com.google.gwt.libideas.StyleInjector'/>
    <inherits name="com.google.gwt.widgetideas.WidgetIdeas"/>
    <inherits name="org.timepedia.chronoscope.gviz.api.GVizChronoscopeAPI"/>

    <entry-point class="org.timepedia.chronoscope.gviz.gadget.client.GVizChronoscopeGadget"/>
    <set-property name="user.agent" value="safari"/>
    <set-property name="selectorCapability" value="js"/>
    <set-property name="hasflash" value="yes"/>
    <set-property name="export" value="no"/>
</module>
    